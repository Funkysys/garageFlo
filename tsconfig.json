{
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"src/pages/api/hello.js",
		"src/pages/index.tsx",
		"src/pages/api/users.js",
		"src/components/Global/Header-Navbar.js",
		"src/components/Global/FeaturesServices/FeaturesServices.jsx",
		"src/components/Global/BossSection/BossSection.jsx",
		"src/components/Global/brandsData",
		"src/components/Global/login/login.tsxx"
	],
	"exclude": ["node_modules"]
}
